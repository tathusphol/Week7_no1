pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }
        stage('read') {
           steps {
               script {
                   def data = readFile(file: 'test.json')
                   echo data.Name
               }
           }
       }
        stage('Tigger the Slave Jobs') {
            steps {
                echo 'Start Trigger to send name and age to slave'
                build job: 'jenkins_slave', parameters: [string(name: 'NAME', value: 'Tathuss'), string(name: 'Ages', value: '21')]
            }
        }
    }
}
